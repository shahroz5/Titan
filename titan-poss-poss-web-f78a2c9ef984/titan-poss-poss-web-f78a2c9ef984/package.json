{
  "name": "poss-web",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start:eposs": "node --max_old_space_size=8192 node_modules/@nrwl/cli/bin/nx.js serve --base-href /eposs/",
    "start:poss": "node --max_old_space_size=8192 node_modules/@nrwl/cli/bin/nx.js serve poss --base-href /poss/",
    "build": "node --max_old_space_size=8192 node_modules/@nrwl/cli/bin/nx.js build  --prod --build-optimizer=true",
    "build:eposs": "node --max_old_space_size=8192 node_modules/@nrwl/cli/bin/nx.js build eposs --prod --build-optimizer=true --base-href /eposs/",
    "build:poss": "node --max_old_space_size=8192 node_modules/@nrwl/cli/bin/nx.js build  poss --prod --build-optimizer=true --base-href /poss/",
    "build:analyze:eposs": "node --max_old_space_size=8192 node_modules/@nrwl/cli/bin/nx.js build  eposs --prod --stats-json",
    "build:analyze:poss": "node --max_old_space_size=8192 node_modules/@nrwl/cli/bin/nx.js build  poss --prod --stats-json",
    "test": "ng test --code-coverage ",
    "test-parallel": "nx run-many --target=test --all --parallel --maxParallel=2 --code-coverage",
    "lint": "nx run-many --all --target=lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "sonar": "sonar-scanner",
    "build:possappdoc": "./node_modules/.bin/compodoc -p apps/poss/tsconfig.json -d dist/documentation/poss",
    "build:epossappdoc": "./node_modules/.bin/compodoc -p apps/eposs/tsconfig.app.json -d dist/documentation/eposs",
    "stats:eposs": "npm run build:analyze:eposs && webpack-bundle-analyzer dist/apps/eposs/stats-es2015.json",
    "stats:poss": "npm run build:analyze:poss && webpack-bundle-analyzer dist/apps/poss/stats-es2015.json",
    "presonar": "ts-node combineFiles.ts",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.9",
    "@angular/compiler": "^10.0.9",
    "@angular/core": "^16.2.1",
    "@angular/forms": "^10.0.9",
    "@angular/material": "^10.1.3",
    "@angular/material-moment-adapter": "^10.1.3",
    "@angular/platform-browser": "^10.0.9",
    "@angular/platform-browser-dynamic": "^10.0.9",
    "@angular/router": "^10.0.9",
    "@ngrx/effects": "^10.0.0",
    "@ngrx/entity": "^10.0.0",
    "@ngrx/router-store": "^10.0.0",
    "@ngrx/store": "^10.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "ag-grid-angular": "^23.2.1",
    "ag-grid-community": "^23.2.1",
    "angular2-hotkeys": "^2.2.0",
    "bootstrap": "^4.4.1",
    "core-js": "^2.6.11",
    "dexie": "^3.2.4",
    "event-source-polyfill": "^1.0.21",
    "js-sha256": "^0.9.0",
    "jsrsasign": "^10.8.6",
    "moment": "^2.24.0",
    "moment-es6": "^1.0.0",
    "ng": "0.0.0",
    "ng-connection-service": "^9.0.3",
    "path": "^0.12.7",
    "qz-tray": "^2.1.1-1",
    "rxjs": "~6.6.2",
    "typescript": "^3.9.7",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^10.0.9",
    "@angular/language-service": "^10.0.9",
    "@compodoc/compodoc": "^1.1.11",
    "@ngrx/store-devtools": "^10.0.0",
    "@nrwl/angular": "^16.7.1",
    "@nrwl/cli": "^15.9.4",
    "@nrwl/workspace": "^16.7.1",
    "@types/glob": "^7.1.1",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash.clonedeep": "^4.5.6",
    "codelyzer": "^5.0.1",
    "concat": "^1.0.3",
    "dotenv": "^6.2.0",
    "eslint": "6.8.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "idb": "^5.0.2",
    "jasmine-core": "^2.99.1",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-html-detailed-reporter": "^2.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "0.0.32",
    "ng-packagr": "^16.2.0",
    "object-sizeof": "^1.6.0",
    "prettier": "^2.0.5",
    "protractor": "^5.4.2",
    "sonar-scanner": "^3.1.0",
    "ts-node": "^7.0.1",
    "tsickle": "^0.38.0",
    "tslint": "6.0.0",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
